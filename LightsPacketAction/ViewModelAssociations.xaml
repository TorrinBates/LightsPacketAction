<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:LightsPacketAction">

    <DataTemplate DataType="{x:Type local:LauncherViewModel}">
        <local:LauncherView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:DisplayViewModel}">
        <local:DisplayView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:ErrorViewModel}">
        <local:ErrorView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:ConfigureViewModel}">
        <local:ConfigureView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:NextViewModel}">
        <local:NextView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:DualButtonViewModel}">
        <local:DualButtonView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:LayoutViewModel}">
        <local:LayoutView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:MessagesViewModel}">
        <local:MessagesView/>
    </DataTemplate>
</ResourceDictionary>