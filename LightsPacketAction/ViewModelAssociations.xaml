<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:LightsPacketAction">

    <DataTemplate DataType="{x:Type local:LauncherViewModel}">
        <local:LauncherView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:DisplayViewModel}">
        <local:DisplayView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:ErrorViewModel}">
        <local:ErrorView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local:YesNoViewModel}">
        <local:YesNoView/>
    </DataTemplate>
</ResourceDictionary>